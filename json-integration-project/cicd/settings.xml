        <server>
            <id>orgRepo</id>
            <username>aws</username>
            <password>env.CODEARTIFACT_AUTH_TOKEN</password>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>orgRepo</id>
            <name>development-artifact-repo-development-artifact-repo</name>
